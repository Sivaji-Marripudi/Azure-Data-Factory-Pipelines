{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-datafactory-mrk"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks'"
		},
		"SqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer'",
			"defaultValue": "Integrated Security=True;Data Source=localhost;Initial Catalog=AdventureWorks2017"
		},
		"SqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer'"
		},
		"sourcedatafriends_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sourcedatafriends'"
		},
		"targetdatafriends_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'targetdatafriends'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myazuredatalakemrkproje.dfs.core.windows.net/"
		},
		"AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0318-135321-20rhevtk"
		},
		"SqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-D7G3HKE\\Sivaji Marripudi"
		},
		"sourcedatafriends_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myazuredatalakemrkproje.dfs.core.windows.net/"
		},
		"targetdatafriends_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myazuredatalakemrkproje.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-datafactory-mrk_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDatabricks_accessToken": {
						"value": "[parameters('AzureDatabricks_accessToken')]"
					},
					"sourcedatafriends_accountKey": {
						"value": "[parameters('sourcedatafriends_accountKey')]"
					},
					"targetdatafriends_accountKey": {
						"value": "[parameters('targetdatafriends_accountKey')]"
					},
					"SqlServer_connectionString": {
						"value": "[parameters('SqlServer_connectionString')]"
					},
					"SqlServer_password": {
						"value": "[parameters('SqlServer_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_properties_typeProperties_existingClusterId')]"
					},
					"sourcedatafriends_properties_typeProperties_url": {
						"value": "[parameters('sourcedatafriends_properties_typeProperties_url')]"
					},
					"targetdatafriends_properties_typeProperties_url": {
						"value": "[parameters('targetdatafriends_properties_typeProperties_url')]"
					},
					"SqlServer_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-datafactory-mrk_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-datafactory-mrk_ArmTemplate_0')]"
			]
		}
	]
}